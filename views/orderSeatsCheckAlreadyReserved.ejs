<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://code.jquery.com/jquery-3.5.1.min.js"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <title>broadway9th</title>

    <script>
      const contentId = new URL(window.location.href).searchParams.get(
        'contentId'
      );

      $(document).ready(function () {
        console.log('1');
        navigateToChooseOrEditSeats(contentId);
        console.log('3');
      });

      async function navigateToChooseOrEditSeats(contentId) {
        $.ajax({
          type: 'GET',
          url: `/order-seats/${contentId}/reservedSeats`,
          success: function (response) {
            console.log('2');
            console.log(response);
            if (response.length > 0) {
              location.href = `/render-order-seats/editSeats?contentId=${contentId}`;
            } else {
              location.href = `/render-order-seats/chooseSeats?contentId=${contentId}`;
            }
          },
        });

        return;
      }
    </script>
  </head>
  <body></body>
</html>
